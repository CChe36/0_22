        writeContorl    writeTime;
        writeInterval   .025;

        fields
        (
            p
        );

        probeLocations
        (
            (0.09 0.011 0) // at end of inlet
            (.015 0 0) // beginning of ramp
            (0.03 .0019987629652023315 0) // start of second ramp
            (0.062 .011 0) // start of flat
            (0.052 .0082 0) // under cowl
        );


    }

    /*
